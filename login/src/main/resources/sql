drop TABLE user;
CREATE TABLE `user` (
  `id` varchar(32) NOT NULL COMMENT '用户id',
  `user_name` varchar(255) DEFAULT NULL,
  `password` varchar(32) DEFAULT NULL COMMENT '密码',
  `login_times` int DEFAULT NULL COMMENT '登录次数',
  `last_login_time` datetime DEFAULT NULL COMMENT '上次登录时间',
  `localtion` datetime DEFAULT NULL COMMENT '地理位置',
  `state` char(1) DEFAULT '1' COMMENT '用户当前状态(是：1 ； 否 ： 0)',
  KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户表';





# 数据源配置中要写清过期时间，避免数据库失效但是连接池依然挂着，会连接不上
# springboot和nacos版本有依赖， 不同版本可能不适配
# http://127.0.0.1:8848/nacos     可访问本地nacos可视化
